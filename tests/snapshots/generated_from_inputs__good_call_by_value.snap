  ret void
}

define i32 @main() {
b0:
  call void @test(i32 1)
  call void @printInt(i32 1)
  ret i32 0
}

define void @test(i32 %0) {
b0:
  %v2 = add i32 %0, 1
  call void @printInt(i32 %0)
  ret void
