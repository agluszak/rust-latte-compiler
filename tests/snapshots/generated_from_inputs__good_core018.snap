---
source: tests/generated_from_inputs.rs
description: "/* test input */\n\nint main() {\n  int x = readInt();\n  string y = readString();\n  string z = readString();\n\n  printInt(x-5);\n  printString(y+z);  \n  return 0 ;\n}"
---
; ModuleID = 'core018'
source_filename = "core018"

%string = type { ptr, i32, i32 }

declare void @memcpy(ptr, ptr, i32)

declare void @printInt(i32)

declare void @printString(ptr)

declare i32 @readInt()

declare ptr @readString()

declare void @error()

define i32 @main() {
b0:
  %readInt = call i32 @readInt()
  %readString = call ptr @readString()
  %readString1 = call ptr @readString()
  %v4 = sub i32 %readInt, 5
  call void @printInt(i32 %v4)
  %buf_ptr = getelementptr inbounds %string, ptr %readString, i32 0, i32 0
  %len_ptr = getelementptr inbounds %string, ptr %readString, i32 0, i32 1
  %buf_ptr2 = getelementptr inbounds %string, ptr %readString1, i32 0, i32 0
  %len_ptr3 = getelementptr inbounds %string, ptr %readString1, i32 0, i32 1
  %buf = load ptr, ptr %buf_ptr, align 8
  %len = load i32, ptr %len_ptr, align 4
  %buf4 = load ptr, ptr %buf_ptr2, align 8
  %len5 = load i32, ptr %len_ptr3, align 4
  %new_len = add i32 %len, %len5
  %mallocsize = mul i32 %new_len, ptrtoint (ptr getelementptr (i8, ptr null, i32 1) to i32)
  %new_buf = tail call ptr @malloc(i32 %mallocsize)
  %new_string = tail call ptr @malloc(i32 ptrtoint (ptr getelementptr (%string, ptr null, i32 1) to i32))
  call void @memcpy(ptr %new_buf, ptr %buf, i32 %len)
  %new_buf_second_part = getelementptr i8, ptr %new_buf, i32 %len
  call void @memcpy(ptr %new_buf_second_part, ptr %buf4, i32 %len5)
  %buf_ptr6 = getelementptr inbounds %string, ptr %new_string, i32 0, i32 0
  %len_ptr7 = getelementptr inbounds %string, ptr %new_string, i32 0, i32 1
  %cap_ptr = getelementptr inbounds %string, ptr %new_string, i32 0, i32 2
  store ptr %new_buf, ptr %buf_ptr6, align 8
  store i32 %new_len, ptr %len_ptr7, align 4
  store i32 %new_len, ptr %cap_ptr, align 4
  call void @printString(ptr %new_string)
  ret i32 0
}

declare noalias ptr @malloc(i32)

